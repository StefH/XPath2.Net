{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch XPath2.Extensions.TestConsoleApp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/tests/XPath2.Extensions.TestConsoleApp/bin/Debug/netcoreapp2.0/XPath2.Extensions.TestConsoleApp.dll",
            "args": [],
            "cwd": "${workspaceRoot}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}